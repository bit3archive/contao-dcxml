								</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="fileTree">
							<xsd:annotation>
								<xsd:documentation xml:lang="de">
									Dateibaum
								</xsd:documentation>
								<xsd:documentation xml:lang="en">
									File tree
								</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="tableWizard">
							<xsd:annotation>
								<xsd:documentation xml:lang="de">
									Tabellenassistent
								</xsd:documentation>
								<xsd:documentation xml:lang="en">
									Table wizard
								</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="listWizard">
							<xsd:annotation>
								<xsd:documentation xml:lang="de">
