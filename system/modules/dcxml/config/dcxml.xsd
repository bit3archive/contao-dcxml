									Optionsassistent
								</xsd:documentation>
								<xsd:documentation xml:lang="en">
									Option wizard
								</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="moduleWizard">
							<xsd:annotation>
								<xsd:documentation xml:lang="de">
									Modulassistent
								</xsd:documentation>
								<xsd:documentation xml:lang="en">
									Module wizard
								</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:enumeration value="checkboxWizard">
							<xsd:annotation>
								<xsd:documentation xml:lang="de">
									Checkbox-Assistent
								</xsd:documentation>
								<xsd:documentation xml:lang="en">
									Checkbox Wizard
								</xsd:documentation>
							</xsd:annotation>
						</xsd:enumeration>
						<xsd:pattern value=".*" />
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>

